
<div class="row">
  <div class="container">


    <ol class="breadcrumb">
      <li> <%= link_to 'Home',root_path %></li>

      <li class="active"> All category</li>


    </ol>
  </div>

</div>
<div class="row">
  <div class="container">
    <% if @listings.present? %>


        <%  @listings.each do |listing| %>
            <div class="col-sm-6 col-lg-offset-3">
              <div class="thumbnail">

                <img src='' class='img-responsive' />

                <div class="caption">
                  <h3> <%=link_to listing.name,listing %></h3>
                  <h3> <%=listing.city %></h3>
                  <h3> <%=listing.state %></h3>
                  <h3><a href=''></a> <small></small></h3>
                  <p>    <%= listing.created_at.strftime('%B %d %Y') %> </p>
                  <p></p>
                  <p><a href="" class="btn btn-primary" role="button">View</a></p>
                </div>
              </div>
            </div>

        <% end %>
    <% else %>
        <span>No Listing Yet</span>
    <% end %>

  </div>
</div>



















